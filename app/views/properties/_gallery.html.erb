<!DOCTYPE html>
<html lang="en">
<head>
  <title>Bootstrap Example</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

</head>
<body>

  <div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
      <div class="item active">
        <img src="https://images.unsplash.com/photo-1449844908441-8829872d2607?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=3d71fee945f7373c8d34bd9ca10239a2&auto=format&fit=crop&w=1050&q=80" alt="Los Angeles" style="width:100%;">
      </div>

      <div class="item">
        <img src="https://images.unsplash.com/photo-1422557279422-316d7de15921?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=109f2b233c35d3f9e4a21760dba4a12b&auto=format&fit=crop&w=1050&q=80" alt="Chicago" style="width:100%;">
      </div>

      <div class="item">
        <img src="https://images.unsplash.com/photo-1430285561322-7808604715df?ixlib=rb-0.3.5&s=f37356fce315191ba7a86d4067b9a8c0&auto=format&fit=crop&w=1050&q=80" alt="New york" style="width:100%;">
      </div>
      <div class="item">
        <img src="https://images.unsplash.com/photo-1493925016752-9ac38daba701?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=c396e437833d26fc1603887578b66202&auto=format&fit=crop&w=1050&q=8" alt="New york" style="width:100%;">
      </div>
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
 

</body>
</html>
