<div  class='propertyPageEmpty col-lg-12'>
  <%= render 'properties/galleryIndex' %>
</div>

<div class='container'>

      <div class='row'>
        
        <%@properties.each do |f|%>
        <div class="col-lg-4">
            <div class='thumbnail'>
              <div class='thumbnail'><%= image_tag f.image.url(:medium) %></div>
                  <div class="caption">
                    <h3><a href="/properties/<%=f.id%>"> <%=f.title%> </a></h3>
                    <p>...</p>
                    <p><a href="/properties/<%=f.id%>" class="btn btn-primary" role="button">View Property</a> <a href="#" class="btn btn-default" role="button">Button</a></p>
                  </div>
              </div>
            </div>
        <%end%>


      </div>
</div>
