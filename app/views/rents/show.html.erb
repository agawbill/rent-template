<h1>Rents#show</h1>
<p>Find me in app/views/rents/show.html.erb</p>

<div class='container'>
  <h1>Property#new</h1>
<% if current_admin%>
<%=form_for @claim do |f| %>
  <div class="form-group">
  Description:    <%=f.text_field :description%>
    </div>
  <div class="form-group">
  Status:    <%=f.text_field :status%>
  </div>
  <div class="form-group">
  Type:    <%=f.text_field :type_of_claim%>
  </div>
  <div class="form-group">
  Price:    <%=f.number_field :price%>
  </div>
  <div class="form-group">
  <%= hidden_field_tag "rent_id", @rent.id %>
</div>
<%=f.submit %>
<% end %>
<%end%>
</end>
